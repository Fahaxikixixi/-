.index_swiper swiper {
    height: 340rpx;
    width:750rpx;
}

.index_swiper swiper image {
    width: 100%;
    height: 340rpx;
}
.index_cate{
    display:flex;
}
.index_cate navigator{
    flex:1;
    padding:20rpx;
   
}
.index_cate navigator image{
    width:100%;
    height: 130rpx;
}
.index_floor{

}
.floor_group{}
.floor_title{
    padding:10rpx 0
}
.floor_title image{
    width: 100%;
    height: 60rpx;
}
.floor_list{
    /* 清除浮动 */
    overflow: hidden;
}
.floor_list navigator{
    float: left;
    /* 每一列占三分之一 */
    width: 33.33%;
}
/* 原图 232*386  第一张
    232/386=33.33vw/height
*/
.floor_list navigator:nth-child(1){
    height: 55.4vw;
}
.floor_list navigator:nth-child(2),
.floor_list navigator:nth-child(3),
.floor_list navigator:nth-child(4),
.floor_list navigator:nth-child(5){
    height: 27.727vw;
    border-left: 10rpx solid #fff;
}
.floor_list navigator:nth-child(2),
.floor_list navigator:nth-child(3){
    border-bottom: 10rpx solid #fff;
}
.floor_list navigator image{
    width: 100%;
    height: 100%;
}